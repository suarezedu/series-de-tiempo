numprof<-sample(5:8,60,replace=TRUE)
crepib<-sample(0:5,60,replace=TRUE)
ocupados<-sample(40:60,60,replace=TRUE)
desocupados<-sample(3:8,60,replace=TRUE)
pceu<-data.frame(numprof,crepib,ocupados,desocupados)
pceu[1:12,]
pceu[49:60,]
View(pceu)
tsprof<-ts(numprof,frequency=12,start=(2010))
tscrepib<-ts(crepib,frequency=12,start=(2010))
tsocupados<-ts(ocupados,frequency=12,start=(2010))
tsdesocupados<-ts(desocupados,frequency=12,start=(2010))
tspceu<-ts(pceu,frequency=12,start=(2010))
start(tsprof);end(tsprof)
###profesionistas y ocupados en la misma grafica
plot(cbind(tsprof,tsocupados))
plot(cbind(tscrepib,tsdesocupados))
plot(cbind(tsprof,tsocupados,tscrepib,tsdesocupados))
plot.ts(pceu[,c(1,3)])
pceusts<-ts.intersect(pceu)

##series de timpo multiple
seriemultiple<-ts.intersect(tsprof,tsocupados,tscrepib,tsdesocupados)
start(seriemultiple)
end(seriemultiple)
#con las 4 variables
plot(seriemultiple,main="serie de tiempo multiple",xlab="años",ylab="numero de personas",type="overploted",pch=2)
seriemultiple10.11<-window(seriemultiple,start=c(2010,1),end=c(2011,1))
seriemultiple11.14<-window(seriemultiple,start=c(2011,1),end=c(2014,12))
plot(seriemultiple10.11,main="serie de tiempo multiple",xlab="años",ylab="numero de personas")
plot(seriemultiple11.14,main="serie de tiempo multiple",xlab="años",ylab="numero de personas")
start(seriemultiple);end(seriemultiple);class(seriemultiple)

#####    Ejercicio    ####
mattel<-read.csv("C:\\Users\\Sala-D1\\Desktop\\mattel.csv")
bimbo<-read.csv("C:\\Users\\Sala-D1\\Desktop\\bimbo.csv")
carso<-read.csv("C:\\Users\\Sala-D1\\Desktop\\carso.csv")
cemex<-read.csv("C:\\Users\\Sala-D1\\Desktop\\cemex.csv")
tsm<-ts(mattel,frequency=1,start=(1980))
tsb<-ts(bimbo,frequency=1,start=(1980))
tscarso<-ts(carso,frequency=1,start=(1980))
tscemex<-ts(cemex,frequency=1,start=(1980))
start(tsm);end(tsm)
plot(cbind(tsm,tsb,tscarso,tscemex))
seriem<-ts.intersect(tsm,tsb,tscarso,tscemex)
start(seriem)
end(seriem)
plot(seriem,main="serie de tiempo multiple",xlab="años",ylab="precios",type="overploted",pch=1)
seriemultiple80.89<-window(seriem,start=c(1980,1),end=c(1989,1))
seriemultiple90.99<-window(seriem,start=c(1990,1),end=c(1999,1))
seriemultiple00.09<-window(seriem,start=c(2000,1),end=c(2009,1))
start(seriemultiple80.89);end(seriemultiple80.89)
plot(seriemultiple80.89,main="serie de tiempo 10",xlab="años",ylab="precio")
plot(seriemultiple90.99,main="serie de tiempo 20",xlab="años",ylab="precio")
plot(seriemultiple00.09,main="serie de tiempo 30",xlab="años",ylab="precio")
