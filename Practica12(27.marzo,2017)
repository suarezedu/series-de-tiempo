asa1<-t(data.frame(read.csv("C:\\Users\\Sala-D1\\Desktop\\IndicadoresENOE_2017-03-27.csv",header=T)[7,2:49]))
View(asa1)
class(asa1)
asat<-as.numeric(asa1)
asat2<-ts(asat,frequency=4,start=2005)
class(asat2)
asat2
###SUAVIZADO EXPONENCIAL EN R
install.packages("forecast")
require(forecast)
mod1<-ses(asat2,alpha=.1,initial="simple",h=8)
mod2<-ses(asat2,alpha=.3,initial="simple",h=8)
mod3<-ses(asat2,alpha=.95,h=8)
x11()
plot(mod1,ylab="asalariados",xlab="aÃ±os",main="asalariados",type="o")
lines(mod1$fitted,col="brown",type="o")
lines(mod2$fitted,col="red",type="o")
lines(mod3$fitted,col="green",type="o")
lines(mod1$mean,col="brown",type="o")
lines(mod2$mean,col="red",type="o")
lines(mod3$mean,col="green",type="o")
legend("topleft",lty=1,col=c(1,"blue","red","green"),c("datos",expression(alpha==.1),expression(alpha==.3),expression(alpha==.95)))
