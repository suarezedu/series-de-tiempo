#fit1<-holt(,alpha=,beta=,initial="simple",h=5)
#fit2<-holt(,alpha=,beta=,initial="simple",exponential=T,h=5)
#fit3<-holt(deso,damped=T)
#fit4<-holt(deso,exponential=T,damped=T)
s1<-c(1,-1,3,2,5,3,7,6,9,8,11,7,13,11,15,20,9,18,16,25)
s2<-c(20,18,21,34,16,1,14,23,12,8,6,4,1,4,2,0,2,1,0,-1)
ts1<-ts(s1,frequency=1,start=2000)
ts2<-ts(s2,frequency=1,start=2000)
ts1.1<-window(ts1,start=2000,end=2015)
ts1.2<-window(ts2,start=2000,end=2015)
##aplicar modelos
library(fpp)
fit1<-holt(ts1.1,alpha=.7,beta=.8,initial="simple",h=5)
fit2<-holt(ts1.1,alpha=.7,beta=.8,initial="simple",exponential=T,h=5)
fit3<-holt(ts1.1,damped=T)
fit4<-holt(ts1.1,exponential=T,damped=T)

x11()
plot(fit1,main="pronostico anual")
lines(fit1$mean,col="blue")
lines(fit2$mean,col="green")
lines(fit3$mean,col="red")
lines(fit4$mean,col="purple")
legend("topleft",lty=1,col=c(1,"blue","green","red","purple"),c("datos",expression(fit==1),expression(fit==2),expression(fit==3),expression(fit==4)))
##mod2 
fi1<-holt(ts1.2,alpha=.7,beta=.8,initial="simple",h=5)
fi2<-holt(ts1.2,alpha=.7,beta=.8,initial="simple",exponential=T,h=5)
fi3<-holt(ts1.2,damped=T)
fi4<-holt(ts1.2,exponential=T,damped=T)

x11()
plot(fi1,main="pronostico anual")
lines(fi1$mean,col="blue")
lines(fi2$mean,col="green")
lines(fi3$mean,col="red")
lines(fi4$mean,col="purple")
legend("topleft",lty=1,col=c(1,"blue","green","red","purple"),c("datos",expression(fit==1),expression(fit==2),expression(fit==3),expression(fit==4)))

