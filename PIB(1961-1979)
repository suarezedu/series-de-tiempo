pib<-read.csv("C:\\Users\\TEMP.RTIC-E30-HP.000\\Downloads\\PIB60-15.csv")
pib2<-pib[1:19,]
pib2
tspib<-ts(pib2,frequency=1,start=1961)
plot(tspib,xlab="años",ylab="pib",main="desarrollo pib",asp=1)
##Observamos que existen tres ciclos, 1969, 1971 y 1977
